[alias]
  a = add
  aa = add .
  ac = "!f() { git add . && git commit -m \"$1\"; }; f"
  b = branch
  bd = branch -d
  c = commit -m
  cl = "!f() { base_url=\"git@github.com:\"; if [[ \"$1\" == *\"/\"* ]]; then git clone \"$base_url/$1.git\"; else git clone \"$base_url/maclong9/$1.git\"; fi; }; f"
  co = checkout
  d = diff
  p = push
  pl = pull
  pln = pull --no-rebase
  s = status
  rl = reset --hard HEAD^
  al = commit --amend --no-edit
